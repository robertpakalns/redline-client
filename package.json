{
  "type": "module",
  "name": "redline-client",
  "version": "1.1.2",
  "description": "Redline Client",
  "author": "robertpakalns",
  "main": "dist/index.js",
  "scripts": {
    "copy": "copyfiles -u 1 \"src-rust/**/*.node\" dist/src-rust && copy-folder-util assets dist/assets",
    "prepare": "tsc && tsc-alias && bun run copy",
    "dev": "bun run prepare && electron .",
    "build": "bun run prepare && electron-builder"
  },
  "license": "ISC",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "@types/electron": "^1.6.12",
    "@types/electron-localshortcut": "^3.1.3",
    "@types/node": "^24.0.14",
    "copy-folder-util": "^1.1.5",
    "copyfiles": "^2.4.1",
    "electron": "^37.2.2",
    "electron-builder": "^26.0.12",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "chart.js": "^4.5.0",
    "electron-localshortcut": "^3.2.1",
    "electron-updater": "^6.6.2"
  },
  "optionalDependencies": {
    "@juice-client/node-enject": "^1.0.4"
  }
}